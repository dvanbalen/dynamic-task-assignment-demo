import java.lang.Number;
import com.gnw.domain.Appraiser;
import com.gnw.domain.Property;

rule "FilterAppraisers"
	dialect "mvel"
	when
		$p : Property( )
		$a : Appraiser( province == $p.address )
		$taskOwner : Appraiser( first == null )
	then
		$taskOwner.setFirst( $a.first );
		update( $taskOwner );
end
